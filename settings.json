{
    // lot of tips from here
    // https: //calebporzio.com/my-vs-code-setup-2
    // "editor.fontFamily": "Inter_,'FiraCode-Light', Menlo, monospace",
    // https://github.com/kiliman/operator-mono-lig
    "editor.fontFamily": "OperatorMonoLig-Book",
    "editor.fontLigatures": true,
    "editor.fontWeight": "200",
    "editor.renderWhitespace": "boundary",
    "editor.fontSize": 15,
    "editor.lineHeight": 40,
    "editor.letterSpacing": 0,
    "editor.suggestFontSize": 15,
    "editor.suggestLineHeight": 28,
    "editor.matchBrackets": "near",
    "editor.lineNumbers": "on",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderLineHighlight": "none",
    "editor.emptySelectionClipboard": false,
    "editor.wordSeparators": "`~!@#%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.quickSuggestionsDelay": 0,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "line-thin",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.renderControlCharacters": true,
    "breadcrumbs.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false, 
    "git.autofetch": true,
    "git.confirmSync": false,
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.statusBar": "tags",
    "todo-tree.highlights.enabled": false,
    "sync.gist": "1be1c589859023df8b6a54f8e1f740f7",
    "sync.autoUpload": true,
    "sync.quietSync": true,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.7,
    "terminal.integrated.cursorBlinking": false,
    "terminal.integrated.cursorStyle": "line",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.activityBar.visible": false,
    "workbench.editor.showTabs": false,
    "workbench.fontAliasing": "antialiased",
    "workbench.colorTheme": "Cobalt2",
    "workbench.sideBar.location": "right",
    "window.zoomLevel": -1,
    "zenMode.hideLineNumbers": true,
    "zenMode.hideActivityBar": false,
    "zenMode.hideTabs": false,
    "zenMode.fullScreen": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "workbench.startupEditor": "none",
    "task.quickOpen.skip": true,
    "window.nativeTabs": true,
    "git.enableSmartCommit": true,
    "timeline.excludeSources": [],
    "editor.largeFileOptimizations": false,
    "jest.autoEnable": false,
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "eslint.alwaysShowStatus": true,
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    }
}